    "@types/underscore.string": "^0.0.38",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.0",
    "npm": "npm is not supported",
    "yarn": ">=1.22"
  },
  "repository": {
    "type": "git",
